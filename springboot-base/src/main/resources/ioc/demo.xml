<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans
        http://www.springframework.org/schema/beans/spring-beans.xsd">

    <bean id="student" class="com.component.ioc.xml.Student">
        <constructor-arg index="0" value="zhangsan"/>
        <constructor-arg index="1" value="13"/>
        <property name="classList">
            <list>
                <value>math</value>
                <value>english</value>
            </list>
        </property>
    </bean>

    <bean id="xmlTest" class="com.component.ioc.xml.App">
        <property name="student" ref="student"/>
        <property name="animal" ref="dog"/>
<!--        <property name="animal" ref="cat"/>-->
    </bean>

<!--    <bean id="dog" factory-bean="animalFactory" factory-method="getAnimal">-->
<!--        <constructor-arg value="dog"/>-->
<!--    </bean>-->

    <bean name="animalFactory" class="com.component.ioc.xml.AnimalFactory"/>
    <bean id="dog" class="com.component.ioc.xml.AnimalFactory" factory-method="getAnimal">
        <constructor-arg value="dog"/>
    </bean>

<!--    <bean id="cat" factory-bean="animalFactory" factory-method="getAnimal">-->
<!--        <constructor-arg value="cat"/>-->
<!--    </bean>-->

</beans>